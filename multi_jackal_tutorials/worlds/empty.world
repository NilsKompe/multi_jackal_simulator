<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://AruCo_Block_0</uri>
      <name>AruCo_Block_0</name>
    </include>
    <include>
      <uri>model://AruCo_Block_1</uri>
      <name>AruCo_Block_1</name>
    </include>
    <include>
      <uri>model://AruCo_Block_2</uri>
      <name>AruCo_Block_2</name>
    </include>
    <include>
      <uri>model://AruCo_Block_3</uri>
      <name>AruCo_Block_3</name>
    </include>
    <!-- Own physics settings to speed up simulation -->
    <physics type='ode'>
      <max_step_size>0.01</max_step_size>
      <real_time_factor>10</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
      <gravity>0 0 -9.8</gravity>
    </physics>
  </world>
</sdf>
